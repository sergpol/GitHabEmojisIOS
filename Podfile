# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'GitHub API emojis' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for GitHub API emojis
  pod 'Alamofire'
  pod 'AlamofireImage'
  pod 'AlamofireNetworkActivityIndicator'
  pod 'EVReflection/Alamofire', :git => 'https://github.com/evermeer/EVReflection.git'
  pod 'SwiftyJSON'
  
  target 'GitHub API emojisTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GitHub API emojisUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
